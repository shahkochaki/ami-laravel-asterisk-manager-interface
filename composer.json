{
    "name": "shahkochaki/ami",
    "description": "A package to connect Laravel servers with VOIP servers on the Issabel platform by AMI",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Shahkochaki",
        "Ami",
        "Asterisk",
        "Issabel",
        "Voip"
    ],
    "homepage": "https://github.com/shahkochaki/ami",
    "authors": [
        {
            "name": "Ali Shahkochaki",
            "email": "ali.shahkochaki7@gmail.com",
            "homepage": "https://shahkochaki.ir",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-mbstring": "*",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/events": "^9.0|^10.0|^11.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "react/dns": "^1.0",
        "react/stream": "^1.0",
        "react/socket": "^1.0",
        "clue/ami-react": "^0.5|^1.0",
        "jackkum/phppdu": "^1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "illuminate/config": "^9.0|^10.0|^11.0",
        "illuminate/container": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Shahkochaki\\Ami\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shahkochaki\\Ami\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpcs"
        ],
        "phpunit": "php vendor/bin/phpunit",
        "phpcs": "php vendor/bin/php-cs-fixer --diff --dry-run -v fix"
    },
    "bin": [
        "bin/ami"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}